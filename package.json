{
  "name": "jsblockhelper",
  "displayName": "jsblockhelper",
  "description": "inserting js instructions round the selected code",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.73.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:jsblockhelper.addtrycatch",
    "onCommand:jsblockhelper.addifelse"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [{
      "command": "jsblockhelper.addtrycatch",
      "title": "Insert try catch"
    },
    {
      "command": "jsblockhelper.addifelse",
      "title": "Insert if else"
    }
  ],
    "keybindings":[
      {
        "command": "jsblockhelper.addtrycatch",
        "key": "ctrl+k+e"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.73.0",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "16.x",
    "eslint": "^8.26.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.8.4",
    "@vscode/test-electron": "^2.2.0"
  }
}
